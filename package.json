{
	"name": "hastepaste-website",
	"version": "1.0.0",
	"description": "🖥️ HastePaste app website",
	"author": {
		"email": "demirci.baris38@gmail.com",
		"name": "Barış DEMİRCİ",
		"url": "https://bariscodes.me"
	},
	"license": "GPL-3.0",
	"scripts": {
		"dev": "next ./src",
		"dev-config": "cross-env DEBUG_CONFIG=true next ./src",
		"build": "next build ./src",
		"build:analyze": "cross-env ANALYZE=true yarpm run build",
		"build:maxify": "cross-env ANALYZE=true MINIFY=false yarpm run build",
		"start": "next start ./dist",
		"test": "jest",
		"check": "tsc --noEmit",
		"lint": "eslint --fix ./src",
		"lint:all": "cross-env DEBUG=eslint:cli-engine eslint --fix --ignore-pattern '!.eslintrc.js' .",
		"lint-config": "eslint --print-config .eslintrc.js",
		"format": "prettier --write ./src",
		"format:all": "prettier --write .",
		"format:watch": "onchange . -- prettier --write {{changed}}",
		"submodule": "git submodule init && git submodule update"
	},
	"dependencies": {
		"@material-ui/core": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.57",
		"autoprefixer": "^9",
		"classnames": "^2.2.6",
		"next": "^10.0.3",
		"next-optimized-images": "^2.6.2",
		"next-seo": "^4.17.0",
		"postcss": "^7",
		"react": "^17.0.1",
		"react-cookie": "^4.0.3",
		"react-cookie-consent": "^6.1.0",
		"react-dom": "^17.0.1",
		"react-syntax-highlighter": "^15.3.1",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.11",
		"@types/enzyme": "^3.10.8",
		"@types/jest": "^26.0.15",
		"@types/node": "^14.14.10",
		"@types/react": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"@zeit/next-css": "^1.0.1",
		"@zeit/next-sass": "^1.0.1",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.14.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"jest": "^26.6.3",
		"less": "^3.12.2",
		"next-compose-plugins": "^2.2.1",
		"next-progressbar": "^1.0.0",
		"node-sass": "^5.0.0",
		"onchange": "^7.1.0",
		"prettier": "^2.2.0",
		"stylelint-config-recommended": "^3.0.0",
		"ts-jest": "^26.4.4",
		"typescript": "^4.1.2",
		"yarpm": "^0.2.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
