	"license": "GPL-3.0",
	"scripts": {
		"dev": "next ./src",
		"dev-config": "cross-env DEBUG_CONFIG=true next ./src",
		"build": "next build ./src",
		"build:analyze": "cross-env ANALYZE=true yarpm run build",
		"build:maxify": "cross-env ANALYZE=true MINIFY=false yarpm run build",
		"start": "next start ./src",
		"test": "jest",
		"check": "tsc --noEmit",
		"lint": "eslint --fix ./src",
		"lint:all": "cross-env DEBUG=eslint:cli-engine eslint --fix --ignore-pattern '!.eslintrc.js' .",
		"lint-config": "eslint --print-config .eslintrc.js",
		"format": "prettier --write ./src",
		"format:all": "prettier --write .",
		"format:watch": "onchange . -- prettier --write {{changed}}"
	},
	"dependencies": {
		"autoprefixer": "^10.0.2",
		"classnames": "^2.2.6",
		"next": "^10.0.3",
		"next-seo": "^4.17.0",
		"postcss": "^8.1.10",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"tailwindcss": "^2.0.1"
	},
	"devDependencies": {
		"@blunck/next-alias": "^1.0.0",
		"@svgr/webpack": "^5.5.0",
		"@types/classnames": "^2.2.11",
		"@types/enzyme": "^3.10.8",
		"@types/jest": "^26.0.15",
