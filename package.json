		"onchange": "^7.1.0",
		"postcss-fixes": "^2.0.1",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.2.0",
		"ts-jest": "^26.4.4",
		"typescript": "^4.1.2",
		"webpack-bundle-analyzer": "^4.1.0",
		"yarpm": "^0.2.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
